{
    "version": 2,
    "buildCommand": "pip install -r requirements.txt",
    "outputDirectory": ".",
    "functions": {
        "api/index.py": {
            "runtime": "python3.9",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "api/index.py"
        }
    ]
}