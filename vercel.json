{
    "version": 2,
    "functions": {
        "api/index.py": {
            "runtime": "python"
        }
    },
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}