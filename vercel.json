{
    "version": 2,
    "buildCommand": "pip install -r requirements.txt",
    "outputDirectory": ".",
    "functions": {
        "api/index.py": {
            "runtime": "python3.9",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "runtime": "python3.9",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}